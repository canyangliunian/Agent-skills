{
  "$schema": "https://opencode.ai/config.json",
  "autoupdate": true,
  "mcp": {
    "Playwright": {
      "command": [
        "npx",
        "-y",
        "@playwright/mcp@latest"
      ],
      "enabled": true,
      "type": "local"
    },
    "Ref": {
      "command": [
        "npx",
        "ref-tools-mcp@latest"
      ],
      "enabled": true,
      "environment": {
        "REF_API_KEY": "ref-6f5673e42e0b75c97c9c"
      },
      "type": "local"
    },
    "chrome_devtools": {
      "command": [
        "npx",
        "chrome-devtools-mcp@latest"
      ],
      "enabled": true,
      "type": "local"
    },
    "comtrade": {
      "command": [
        "uvx",
        "comtrade-mcp-server"
      ],
      "enabled": true,
      "environment": {
        "COMTRADE_SUBSCRIPTION_KEY": "78035232d8da48c1b27b854a61764f91"
      },
      "type": "local"
    },
    "context7": {
      "command": [
        "npx",
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "enabled": true,
      "type": "local"
    },
    "exa": {
      "command": [
        "npx",
        "-y",
        "exa-mcp-server",
        "tools=web_search_exa,get_code_context_exa"
      ],
      "enabled": true,
      "type": "local"
    },
    "latex-server": {
      "command": [
        "python",
        "/Users/lingguiwang/mcp-latex-server/latex_server.py"
      ],
      "enabled": true,
      "environment": {
        "LATEX_BASE_PATH": "/Users/lingguiwang/latex"
      },
      "type": "local"
    },
    "mcp-deepwiki": {
      "command": [
        "npx",
        "-y",
        "mcp-deepwiki@latest"
      ],
      "enabled": true,
      "type": "local"
    },
    "notebooklm": {
      "command": [
        "npx",
        "-y",
        "notebooklm-mcp@latest"
      ],
      "enabled": true,
      "type": "local"
    },
    "stata-han": {
      "command": [
        "mcp-proxy",
        "http://localhost:4000/mcp"
      ],
      "enabled": true,
      "type": "local"
    },
    "stata-sep": {
      "command": [
        "uvx",
        "stata-mcp"
      ],
      "enabled": true,
      "type": "local"
    }
  },
  "model": "google/gemini-3-pro-preview",
