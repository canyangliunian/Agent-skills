{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    "sisyphus": {
      "model": "nvidia/z-ai/glm4.7",
      "description": "执行者",
      "temperature": 0.2
    },
    "oracle": {
      "model": "openai/gpt-5.2",
      "description": "复杂架构与策略分析",
      "temperature": 0.1,
      "permission": {
        "edit": "allow",
        "bash": "ask",
        "webfetch": "ask",
        "external_directory": "deny"
      },
      "variant": "high"
    },
    "librarian": {
      "model": "opencode/glm-4.7-free",
      "description": "多库文档/代码搜索",
      "permission": {
        "edit": "deny",
        "bash": "deny",
        "webfetch": "allow",
        "external_directory": "deny"
      }
    },
    "explore": {
      "model": "opencode/claude-haiku-4-5",
      "description": "代码库探索",
      "permission": {
        "edit": "deny",
        "bash": "ask",
        "webfetch": "allow",
        "external_directory": "deny"
      }
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash",
      "description": "多模态内容理解",
      "permission": {
        "edit": "deny",
        "bash": "deny",
        "webfetch": "deny"
      }
    },
    "prometheus": {
      "model": "github-copilot/claude-opus-4.5",
      "variant": "max"
    },
    "metis": {
      "model": "github-copilot/claude-opus-4.5",
      "variant": "max"
    },
    "momus": {
      "model": "openai/gpt-5.2",
      "variant": "medium"
    },
    "atlas": {
      "model": "opencode/kimi-k2.5-free"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "google/gemini-3-pro"
    },
    "ultrabrain": {
      "model": "openai/gpt-5.2-codex",
      "variant": "xhigh"
    },
    "deep": {
      "model": "openai/gpt-5.2-codex",
      "variant": "medium"
    },
    "artistry": {
      "model": "google/gemini-3-pro",
      "variant": "max"
    },
    "quick": {
      "model": "github-copilot/claude-haiku-4.5"
    },
    "unspecified-low": {
      "model": "github-copilot/claude-sonnet-4.5"
    },
    "unspecified-high": {
      "model": "github-copilot/claude-sonnet-4.5"
    },
    "writing": {
      "model": "google/gemini-3-flash"
    }
  }
}